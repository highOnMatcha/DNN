      "elastic_transform": {"alpha": 0, "sigma": 0, "p": 0.0},
      "random_perspective": {"distortion_scale": 0.08, "p": 0.15},
      "random_erasing": {"p": 0.0, "scale": [0.0, 0.0], "ratio": [0.0, 0.0]},
      "mixup": {"alpha": 0.0, "p": 0.0},
      "random_crop_scale": {"scale": [0.92, 1.0], "p": 0.3},
