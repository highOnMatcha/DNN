      "brightness_contrast": {"brightness_factor": [0.8, 1.2], "contrast_factor": [0.8, 1.2], "p": 0.4},
      "channel_shuffle": {"p": 0.08},
      "vertical_flip_p": 0.15,
      "random_affine": {"degrees": 5, "translate": [0.05, 0.05], "scale": [0.95, 1.05], "p": 0.2}
