        "discriminator": {
          "input_channels": 3,
          "ndf": 32,
          "n_layers": 3,
          "norm_layer": "instance"
        },
        "image_size": 64,
        "lambda_cycle": 10.0,
        "lambda_identity": 0.5,
        "beta1": 0.5,
        "beta2": 0.999
