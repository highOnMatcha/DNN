    "ddpm-medium": {
      "name": "ddpm-medium",
      "description": "Medium DDPM model (25M parameters) - Better quality diffusion model",
      "output_dir": "./models/ddpm_medium",
      "architecture": "ddpm",
      "parameters": {
        "channels": 3,
        "image_size": 64,
        "num_res_blocks": 3,
        "attention_resolutions": [32, 16, 8],
        "channel_mult": [1, 2, 4, 8],
        "num_heads": 4,
        "use_scale_shift_norm": true,
        "dropout": 0.1,
        "timesteps": 1000,
        "noise_schedule": "cosine",
        "loss_type": "l2"
      }
    }
