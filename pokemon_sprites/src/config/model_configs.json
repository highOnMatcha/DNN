    "anti_overfitting": {
      "horizontal_flip_p": 0.8,
      "rotation_degrees": 8,
      "color_jitter": {
        "input": {"brightness": 0.2, "contrast": 0.2, "saturation": 0.25, "hue": 0.08},
        "target": {"brightness": 0.08, "contrast": 0.08, "saturation": 0.12, "hue": 0.03}
